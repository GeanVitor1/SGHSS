<form id="formIncluirContato" class="text-start">
    <div class="form-group form-group-default mt-3">
        <label>Contato</label>
        <input id="Contato" name="Contato" type="text" class="form-control" required />
    </div>
    <div class="form-group form-group-default mt-3">
        <label>Tipo Contato</label>
        <select class="form-select" id="Tipo" name="Tipo">
            <option value=""></option>
            <option value="Telefone Fixo">Telefone Fixo</option>
            <option value="Telefone Celular">Telefone Celular</option>
            <option value="Email">Email</option>
            <option value="Contato de Terceiro">Contato de Terceiro</option>
        </select>
    </div>
    <div class="form-check form-switch mt-2">
        <input class="form-check-input c-pointer" type="checkbox" name="IsWhatsApp" role="switch" id="IsWhatsApp" value="true">
        <label class="form-check-label c-pointer" for="IsWhatsApp">É WhatsApp</label>
    </div>
</form>